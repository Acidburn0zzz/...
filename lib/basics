PATH="$HOME/.../bin:$PATH" # needed to bootsrap
eval `... env`
source ~/.../lib/tracefuncs
...debug2 "Did eval `... env`, producing: `... env`"

source ~/.../lib/scripting
source ~/.../lib/path-manip

...filestart "~/.../lib/basics" # Bash won't let me use "$0" here.
...source ~/.../lib/path-manip
...path-prepend ~/.../bin
...path-prepend ~/bin

...source ~/.../lib/set-shellname # makes $SHELLNAME = "zsh" or "bash"

...fileend "~/.../lib/basics"
# vim:ft=zsh
